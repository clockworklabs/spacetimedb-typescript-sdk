{
  "name": "vanilla-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "spacetime:generate-bindings": "spacetime generate --lang typescript --out-dir ./src/module_bindings --project-path ./server",
    "spacetime:start": "spacetime start server",
    "spacetime:publish:local": "spacetime publish playground-chat-vanilla-ts2 --project-path ./server --server 0.0.0.0:80"
  },
  "dependencies": {
    "@clockworklabs/spacetimedb-sdk": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
